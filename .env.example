APP_NAME=Laravel
APP_ENV=local
APP_KEY=your-app-key-here
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=en
APP_TIMEZONE=UTC
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=manga_inertia
DB_USERNAME=root
DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=720
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# SEO Configuration
SEO_SITE_NAME="Your Manga Site"
SEO_DEFAULT_TITLE="Default Manga Title"
SEO_DEFAULT_DESCRIPTION="Default description for your manga site."
SEO_DEFAULT_KEYWORDS="manga, anime, read manga online"
SEO_DEFAULT_IMAGE="/images/default-og.jpg"
SEO_LOCALE="en_US"
SEO_OG_TYPE="website"
SEO_TWITTER_HANDLE="@mangaonline"

# Manga Page SEO Configuration
SEO_MANGA_INDEX_TITLE="Manga List - Your Manga Site"
SEO_MANGA_INDEX_DESCRIPTION="Discover your favorite manga from popular to latest releases. Search by genre, ranking, and new updates."
SEO_MANGA_INDEX_KEYWORDS="manga list, manga, popular manga, latest manga, manga ranking"

# Manga Detail SEO Configuration
SEO_MANGA_SHOW_TITLE="{manga_name} - Read Manga Online"
SEO_MANGA_SHOW_DESCRIPTION="{manga_name} - Read Manga for free. Discover your favorite manga from popular to latest releases. Search by genre, ranking, and new updates."
SEO_MANGA_SHOW_KEYWORDS="{manga_name}, {manga_genres}, manga, read manga online"

# Chapter Detail SEO Configuration
SEO_CHAPTER_SHOW_TITLE="{manga_name} - Read Chapter {chapter_title}"
SEO_CHAPTER_SHOW_DESCRIPTION="{manga_name} - Read Chapter {chapter_title} for free. Latest chapter updates, high-quality mobile support."
SEO_CHAPTER_SHOW_KEYWORDS="{manga_name}, 第{chapter_number}話, {manga_genres}, manga, read manga online"

# Search SEO Configuration
SEO_SEARCH_TITLE="Manga Search - {query}"
SEO_SEARCH_DESCRIPTION="Search results for manga '{query}'. {count} titles found. Looking for free manga to read?"
SEO_SEARCH_KEYWORDS="manga search, {query}, manga, read manga online"

# Home SEO Configuration
SEO_HOME_TITLE="Manga - Latest Manga Free Online"
SEO_HOME_DESCRIPTION="Read the latest manga for free online. Discover popular works, new updates, and recommended manga every day. Mobile and PC compatible."
SEO_HOME_KEYWORDS="manga, manga, latest manga, popular manga, free manga"

# SEO Robots Configuration
SEO_ROBOTS_INDEX=true
SEO_ROBOTS_FOLLOW=true
SEO_ROBOTS_MAX_IMAGE_PREVIEW="large"
SEO_ROBOTS_MAX_SNIPPET=-1
SEO_ROBOTS_MAX_VIDEO_PREVIEW=-1

# SEO Schema Configuration
SEO_SCHEMA_LOGO_PATH="/logo.svg"
SEO_SCHEMA_SOCIAL_MEDIA_LINKS=""

# SEO Labels Configuration
SEO_LABEL_HOME="Home"
SEO_LABEL_MANGA="Manga"
SEO_LABEL_CHAPTER="Chapter"
SEO_CHAPTER_DESCRIPTION_PREFIX="Read"
SEO_CHAPTER_DESCRIPTION_SUFFIX="Chapter"

# Server Configuration for Octane, using FrankenPHP or Octane
OCTANE_SERVER=frankenphp

GOOGLE_ANALYTICS_ID=G-XXXXXXXXXXX

# Image URL Encryption Configuration, must be a 16-character key, must fill in with your own key, if not set, the APP will not work properly
IMAGE_ENCRYPTION_KEY=SecretKey1234567